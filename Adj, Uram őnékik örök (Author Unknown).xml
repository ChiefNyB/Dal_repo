<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2020-02-28T13:53:15">
  <properties>
    <titles>
      <title>Adj, Uram őnékik örök</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Adj, Uram őnékik örök nyugodalmat,<br/>és az örökkévaló világosság fényeskedjék néki(k)!</lines>
    </verse>
    <verse name="v2">
      <lines>Hallgasd meg, Uram imádságomat,<br/>minden test Hozzád mégyen.</lines>
    </verse>
  </lyrics>
</song>
