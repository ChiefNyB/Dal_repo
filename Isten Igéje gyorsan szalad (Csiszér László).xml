  <properties>
    <titles>
      <title>Isten Igéje gyorsan szalad</title>
      <title>Timóteus k. himnusz</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author type="words">Csiszér László</author>
      <author type="music">Csiszér László</author>
    </authors>
    <songbooks>
      <songbook name="Szent András énekfüzet" entry="240"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Isten Igéje gyorsan szalad, jöjj hát, te is add tovább!<br/>Isten Igéje mindent áthat, jöjj hát, te is add tovább!</lines>
    </verse>
    <verse name="c1">
      <lines>Az Ige gyorsan fut, tanuld meg jól, és add tovább!<br/>Az Ige gyorsan fut, forgasd szívedben, és add tovább!</lines>
    </verse>
  </lyrics>
</song>
