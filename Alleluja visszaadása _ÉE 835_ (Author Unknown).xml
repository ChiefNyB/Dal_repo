    <titles>
      <title>Alleluja visszaadása [ÉE 835]</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>A vágyva-vágyott éneket ez éj visszaadja,<br/>mert föltámadt Krisztus.<br/>Hát figyeljetek reám, s vegyétek át tőlem,<br/>hogy együtt zenghessük:</lines>
    </verse>
    <verse name="v2">
      <lines>Alleluja!</lines>
    </verse>
  </lyrics>
</song>
