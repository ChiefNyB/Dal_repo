  <properties>
    <titles>
      <title>Istenem, jöjj segítségemre</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="142"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Istenem, jöjj segítségemre,<br/>Uram, segíts meg engem!</lines>
    </verse>
    <verse name="v2">
      <lines>Dicsőség az Atyának, a Fiúnak<br/>és a Szentlélek Istennek,<br/>miképpen kezdetben,<br/>most és mindörökké! Ámen!</lines>
    </verse>
  </lyrics>
</song>
