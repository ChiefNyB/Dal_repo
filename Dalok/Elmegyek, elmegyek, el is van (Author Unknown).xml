<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.1" modifiedIn="OpenLP 3.1.1" modifiedDate="2025-03-10T16:41:20">
  <properties>
    <titles>
      <title>Elmegyek, elmegyek, el is van</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Cserkészek daloskönyve" entry="133"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Elmegyek, elmegyek, el is van vágyásom,<br/>Ebbe' rongyos kis tanyába' nincsen maradásom.</lines>
    </verse>
    <verse name="v2">
      <lines>Ha elmégy, ha elmégy, csak hozzám igaz légy,<br/>Igaz szereteted, babám, hamisra ne fordítsd!</lines>
    </verse>
  </lyrics>
</song>
