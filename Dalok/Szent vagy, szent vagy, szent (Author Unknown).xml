    <verseOrder>v1</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="497"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Szent vagy, szent vagy, szent vagy, mindenség Ura, Istene!<br/>Dicsőséged betölti a mennyet és a földet!<br/>Hozsanna a magasságban!<br/>Áldott, aki jön az Úr nevében!<br/>Hozsanna a magasságban!</lines>
    </verse>
  </lyrics>
</song>
