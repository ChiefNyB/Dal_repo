  <properties>
    <titles>
      <title>I Love You Lord</title>
      <title>gg</title>
    </titles>
    <authors>
      <author>old</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>I love you Lord<br/>and I lift my voice<br/>to worship You<br/>O my soul, rejoice<br/><br/></lines>
    </verse>
    <verse name="v1b">
      <lines>Take joy my King, <br/>in what You hear<br/>let it be a sweet, sweet sound<br/>in Your ear</lines>
    </verse>
  </lyrics>
</song>
