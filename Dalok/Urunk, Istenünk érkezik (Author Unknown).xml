  <properties>
    <titles>
      <title>Urunk, Istenünk érkezik</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="4"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Urunk, Istenünk érkezik:<br/>* Ő a dicsőség királya.</lines>
    </verse>
    <verse name="v2">
      <lines>Hadd énekeljem örökké<br/>* Urunknak irgalmas jóságát!</lines>
    </verse>
    <verse name="v3">
      <lines>Téríts meg minket, Istenünk,<br/>* ragyogtasd ránk arcodat, és szabadok leszünk!</lines>
    </verse>
  </lyrics>
</song>
