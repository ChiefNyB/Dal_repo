  <properties>
    <titles>
      <title>Ad te Jesu Christe</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Taize-i énekek" entry="11"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ad te Jesu Christe<br/>levavi animam meam.<br/>Salvator mundi,<br/>in te speravi.</lines>
    </verse>
    <verse name="v2">
      <lines>Megváltó Úr Jézus,<br/>világot ölelő jóság,<br/>segíts ma rajtam,<br/>emelj föl engem.</lines>
    </verse>
  </lyrics>
</song>
