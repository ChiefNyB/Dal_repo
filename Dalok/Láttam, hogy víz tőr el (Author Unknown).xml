    <titles>
      <title>Láttam, hogy víz tőr el</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="403"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Láttam, hogy víz tőr elő a templomból,<br/>annak jobb oldaláról,<br/>s kikhez elért e vízár,<br/>megszabadultak, és énekelték:<br/>alleluja, alleluja.</lines>
    </verse>
  </lyrics>
