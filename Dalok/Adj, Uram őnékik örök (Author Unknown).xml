  <properties>
    <titles>
      <title>Adj, Uram őnékik örök</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="687"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Adj, Uram őnékik örök nyugodalmat,<br/>és az örökkévaló világosság fényeskedjék néki(k)!</lines>
    </verse>
    <verse name="v2">
      <lines>Hallgasd meg, Uram imádságomat,<br/>minden test Hozzád mégyen.</lines>
    </verse>
  </lyrics>
</song>
