<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.2" modifiedIn="OpenLP 3.1.2" modifiedDate="2025-04-17T16:11:18">
  <properties>
    <titles>
      <title>Wielbic imie Pana dzis chce</title>
    </titles>
    <authors>
      <author>17</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Wielbić imię Pana dziś chcę,  /x3<br/>To mój Bóg. 			     /x2<br/><br/></lines>
    </verse>
    <verse name="v2">
      <lines>Imię Pana jest twierdzą,<br/>warowną twierdzą,<br/>skałą zbawienia, schronieniem mym.   /x2<br/></lines>
    </verse>
  </lyrics>
</song>
