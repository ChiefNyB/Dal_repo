<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.1" modifiedIn="OpenLP 3.1.1" modifiedDate="2025-03-10T16:41:41">
  <properties>
    <titles>
      <title>Menjetek be kapuin - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Hm----------------------------<br/>Menjetek be kapuin hálaadással,</lines>
    </verse>
    <verse name="v1b">
      <lines>-------A----------------Hm<br/>tornácain át dícséretekkel.</lines>
    </verse>
    <verse name="v1c">
      <lines>-----Hm------A---------------Hm--<br/>Mert jó az Úr, örökké való kegyelme</lines>
    </verse>
    <verse name="v1d">
      <lines>-----------A------------------Hm<br/>és hűsége megmarad mindörökre.</lines>
    </verse>
  </lyrics>
</song>
