  <properties>
    <titles>
      <title>Jöjjetek, imádjuk az Istent</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="375"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jöjjetek, imádjuk az Istent,<br/>boruljunk le az Úr előtt.</lines>
    </verse>
    <verse name="v2">
      <lines>Hozzatok áldozatot, és lépjetek be az Úr csarnokába,<br/>(*)<br/>imádjátok Őt szent ünnepi díszben!</lines>
    </verse>
  </lyrics>
</song>
