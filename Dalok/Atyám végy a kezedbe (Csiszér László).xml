  <properties>
    <titles>
      <title>Atyám végy a kezedbe</title>
    </titles>
    <authors>
      <author>Csiszér László</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Atyám végy a kezedbe, tégy eggyé minket<br/>Szentlélek árassz el, tégy eggyé minket<br/>Szentlélek formálj át, tégy eggyé minket</lines>
    </verse>
  </lyrics>
