      <title>Táplálj minket</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Kék Könyv" entry="200"/>
    </songbooks>
