<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.1" modifiedIn="OpenLP 3.1.1" modifiedDate="2025-03-10T16:41:20">
  <properties>
    <titles>
      <title>Eljegyezlek magamnak</title>
    </titles>
    <authors>
      <author type="words">Csiszér László</author>
      <author type="music">Csiszér László</author>
    </authors>
    <songbooks>
      <songbook name="Szent András énekfüzet" entry="27"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Eljegyezlek magamnak igazsággal, szeretettel,<br/>eljegyezlek örökre irgalommal, kegyelemmel.</lines>
    </verse>
    <verse name="v1b">
      <lines>Eljegyezlek magamnak hittel,<br/>igen, hittel jegyezlek el!<br/>És megismered a te Urad,<br/>megismered Istenedet!</lines>
    </verse>
  </lyrics>
</song>
