    <titles>
      <title>A budai cserkészek</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>A budai cserkészek tábort vertek szegények,<br/>Addig verték, agyalták, agyalták, agyalták,<br/>Míg szétmentek a balták.</lines>
    </verse>
  </lyrics>
