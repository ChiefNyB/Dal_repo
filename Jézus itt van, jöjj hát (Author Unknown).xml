  <properties>
    <titles>
      <title>Jézus itt van, jöjj hát</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Zöld könyv" entry="356"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>‖: Jézus itt van, jöjj hát!<br/>Szentlélek szállj ránk!<br/>Atyánkat imádjuk,<br/>a kegyelem áradjon szét! :‖</lines>
    </verse>
    <verse name="v2">
      <lines>Tudom régóta már,<br/>Te vagy a Király: Jézus!<br/>Szívem befogadott,<br/>én a Tied vagyok!<br/>Maradj hát velem!</lines>
    </verse>
  </lyrics>
</song>
