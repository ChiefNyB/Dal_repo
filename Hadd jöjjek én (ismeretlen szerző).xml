  <properties>
    <titles>
      <title>Hadd jöjjek én</title>
    </titles>
    <verseOrder>v1 c1 v2 c1 v3 c1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Sárga könyv" entry="61"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Hadd jöjjek én forrásodhoz, Uram!</lines>
    </verse>
    <verse name="c1">
      <lines>Hadd jöjjek én, hadd igyak én,<br/>hadd éljek én!</lines>
    </verse>
    <verse name="v3">
      <lines>Hadd éljek én forrásodnál, Uram!</lines>
    </verse>
    <verse name="v2">
      <lines>Hadd igyak én forrásodból, Uram!</lines>
    </verse>
  </lyrics>
</song>
